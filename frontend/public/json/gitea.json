  "interface_port": 3000,
  "documentation": "https://docs.gitea.com/",
  "website": "https://gitea.com",
  "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons@main/webp/gitea.webp",
  "config_path": "/etc/gitea/app.ini",
  "description": "Gitea 是一个自托管的 Git 服务。它提供了一个轻量级且易于安装的解决方案来管理 Git 仓库。用户可以协作编写代码、跟踪问题并管理项目任务。Gitea 包含拉取请求、代码审查、wiki 和项目管理工具等功能。它适合寻求控制自己 Git 托管的中小型团队。",
  "install_methods": [
    {
      "type": "default",
      "script": "ct/gitea.sh",
      "resources": {
        "cpu": 1,
        "ram": 1024,
        "hdd": 8,
        "os": "debian",
        "version": "13"
      }
    },
    {
      "type": "alpine",
      "script": "ct/alpine-gitea.sh",
      "resources": {
        "cpu": 1,
        "ram": 256,
        "hdd": 1,
        "os": "alpine",
        "version": "3.23"
      }
    }
  ],
  "default_credentials": {
    "username": null,
    "password": null
  },
  "notes": [
    {
      "text": "The script installs SQLite3 as default database provider. You will be asked to select database type when you initially open the application URL. Select SQLite3 or provide your own database.",
      "type": "info"
    }
  ]
}
