        "cpu": 2,
        "ram": 2048,
        "hdd": 6,
        "os": "ubuntu",
        "version": "24.04"
      }
    }
  ],
  "default_credentials": {
    "username": "admin",
    "password": "`cat /etc/inventree/admin_password.txt`"
