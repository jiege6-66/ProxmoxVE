{
  "name": "Syncthing",
  "slug": "syncthing",
  "categories": [
    12
  ],
  "date_created": "2024-05-02",
  "type": "ct",
  "updateable": true,
  "privileged": false,
  "interface_port": 8384,
  "documentation": "https://docs.syncthing.net/",
  "website": "https://syncthing.net/",
  "logo": "https://cdn.jsdelivr.net/gh/selfhst/icons@main/webp/syncthing.webp",
  "config_path": "/root/.local/state/syncthing/config.xml - Alpine: /var/lib/syncthing/.local/state/syncthing/config.xml",
  "description": "Syncthing 是一个开源文件同步工具,允许用户通过点对点同步在多个设备之间保持文件同步。它不依赖任何中央服务器,因此所有数据传输都直接在设备之间进行。",
  "install_methods": [
    {
      "type": "default",
      "script": "ct/syncthing.sh",
      "resources": {
        "cpu": 2,
        "ram": 2048,
        "hdd": 8,
        "os": "debian",
        "version": "13"
      }
    },
    {
      "type": "alpine",
      "script": "ct/alpine-syncthing.sh",
      "resources": {
        "cpu": 1,
        "ram": 256,
        "hdd": 1,
        "os": "alpine",
        "version": "3.23"
      }
    }
  ],
  "default_credentials": {
    "username": null,
    "password": null
  },
  "notes": []
}
