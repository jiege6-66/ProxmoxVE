      "resources": {
        "cpu": 2,
        "ram": 4096,
        "hdd": 7,
        "os": "debian",
        "version": "13"
      }
    }
  ],
  "default_credentials": {
    "username": null,
    "password": null
  },
  "notes": [
    {
      "text": "Meilisearch-UI can optionally be installed for a web-based search interface",
